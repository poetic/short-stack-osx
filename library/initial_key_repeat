#!/bin/bash

# Exit immediately on error
set -e

# Get params
#source ${1}

# Set defaults
value=${value:-6}

if [[ $(defaults read -g InitialKeyRepeat) == $value ]]; then
  echo '{"changed":false}'
else
  defaults write -g InitialKeyRepeat -int $value
  echo '{"changed":true, "msg":"OK"}'
fi

exit 0
